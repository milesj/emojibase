    "test-shortcodes.json"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "emojibase": "*"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "bundle": true,
    "format": "lib",
    "platform": "node"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  }
}
