{
  "name": "emojibase-test-utils",
  "version": "7.0.0",
  "description": "Test utilities for working with Emojibase data.",
  "keywords": [
    "emoji",
    "emojibase",
    "tests",
    "utils"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/emojibase.git",
    "directory": "packages/test-utils"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}",
    "test-data.json",
