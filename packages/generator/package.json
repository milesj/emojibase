{
  "private": true,
  "name": "emojibase-generator",
  "version": "3.1.2",
  "description": "Scripts for generating emoji datasets.",
  "keywords": [
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/emojibase.git",
    "directory": "packages/generator"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "dependencies": {
    "@types/cheerio": "^0.22.28",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.0.1",
    "@types/node-fetch": "^2.5.10",
    "chalk": "^4.1.1",
    "cheerio": "^1.0.0-rc.6",
    "fs-extra": "^9.1.0",
    "kuroshiro": "^1.1.2",
    "kuroshiro-analyzer-kuromoji": "^1.1.0",
    "node-fetch": "^2.6.1",
    "pofile": "^1.1.1",
    "regexgen": "^1.3.0",
    "transliteration": "^2.2.0"
  },
  "devDependencies": {
    "emojibase": "^5.1.1"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "emoticons": "src/resources/emoticons.ts",
      "shortcodes": "src/resources/shortcodes.ts"
    },
    "platform": "node"
  },
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.1.0"
  }
}
